<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DuetMobile.signUpPage">
    <ContentPage.Content>
        <StackLayout>
           
            <!--Enter School Email-->
            <Label Text="What's your university email?:" />
            <Entry Text= "" BackgroundColor="White" />

            <!--Enter First Name -->
            <Label Text="What's your first name?:" />
            <Entry Text="" BackgroundColor="White" />

            <!--Enter Last Name -->
            <Label Text="What's your last name?:" />
            <Entry Text="" BackgroundColor="White" />

            <!--Enter Gender -->
            <Label Text="What gender do you identify as?:" />
            <Entry Text="" BackgroundColor="White" />

            <!--Enter Year of Birth -->
            <Label Text="What year were you born?:" />
            <Entry Text="" BackgroundColor="White" />

            <!--Button to Create Account and go to survey -->
            <Button Text="Create Account" Clicked="accountCreated"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>